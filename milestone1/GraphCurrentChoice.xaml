<UserControl x:Class="milestone1.GraphCurrentChoice"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:milestone1"
             xmlns:oxy="http://oxyplot.org/wpf"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <oxy:PlotView
                x:Name="CurrentChoice"
                d:DataContext="{d:DesignInstance Type={x:Type local:FlightGearViewModel}}"
                Model="{Binding VM_PlotModelCurrent}">
        </oxy:PlotView>
    </Grid>
</UserControl>

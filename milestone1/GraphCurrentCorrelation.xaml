<UserControl
    x:Class="milestone1.GraphCurrentCorrelation"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:milestone1"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:oxy="http://oxyplot.org/wpf"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid>
        <oxy:PlotView
            x:Name="CurrentCorrelation"
            d:DataContext="{d:DesignInstance Type={x:Type local:FlightGearViewModel}}"
            Model="{Binding VM_PlotModelCurrentCorrelation}" />
    </Grid>
</UserControl>
